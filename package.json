{
    "name": "@semiconscious/pjsua2.js",
    "description": "Native node module for pjsua2 library",
    "version": "0.0.5",
    "main": "dist/binding.js",
    "types": "dist/binding.d.ts",
    "scripts": {
        "pretest": "tsc",
        "test": "node --napi-modules ./test/test_binding.js",
        "build:dev": "tsc",
        "install": "node-pre-gyp install --verbose --fallback-to-build"
    },
    "gypfile": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SemiConscious/pjsua2.js.git"
    },
    "keywords": [
        "pjsip",
        "pjlib",
        "pjsua",
        "pjsua2",
        "telephony",
        "napi",
        "native"
    ],
    "author": "jim.page@redmatter.com",
    "license": "SEE LICENSE IN license",
    "bugs": {
        "url": "https://github.com/SemiConscious/pjsua2.js/issues"
    },
    "homepage": "https://github.com/SemiConscious/pjsua2.js#readme",
    "dependencies": {
        "@mapbox/node-pre-gyp": "^1.0.11",
        "node-addon-api": "^8.0.0"
    },
    "devDependencies": {
        "@types/node": "^20.12.7",
        "node-pre-gyp-github": "^1.4.4",
        "typescript": "^5.4.5"
    },
    "binary": {
        "module_name": "pjsua2",
        "module_path": "./dist",
        "host": "https://github.com/SemiConscious/pjsua2.js/releases/download/",
        "remote_path": "{version}"
    }
}